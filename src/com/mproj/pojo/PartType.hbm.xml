<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.mproj.pojo.PartType" table="T_PARTTYPES">
        <id name="id" type="java.lang.Integer">
            <column name="P_TYPE_ID" />
            <generator class="native" />
        </id>
        
        <!-- 
        <property name="num" column="P_TYPE_NUM" type="java.lang.String">
        </property>
         -->
        
        <property name="name" column="P_TYPE_NAME" type="java.lang.String">
        </property>
        
        <set name="parts" table="T_PARTS" inverse="true">
            <key><column name="P_TYPE_ID" /></key>
            <one-to-many class="com.mproj.pojo.Part" />
        </set>
        
    </class>
</hibernate-mapping>
